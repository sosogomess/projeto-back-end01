generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model HairTone {
  id        Int     @id @default(autoincrement())
  name      String
  image     String
  category  String
  favorites Boolean @default(false)

  @@map("hair_tones")
}
